
{  
  "manifest_version":2,
  "name": "Groopen",  
  "version": "0.5.1", 
  "description": "Group websites and batch-open them!",
  "options_page":"options.html",
  "permissions":["tabs","storage","http://*/"],
  "icons":{"16":"icon16.png","48":"icon48.png"},
  "browser_action": {  
    "default_icon": "icon16.png" ,
    "default_title": "Groopen",
    "default_popup": "popup.html"
  }
} 